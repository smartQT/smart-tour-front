{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^u2-(.*)": "@/components/u-$1/u-$1.vue",
      "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue",
      "^trip-(.*)": "@/components/trip-$1/trip-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/tabbar/home/index",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTextStyle": "white"
      }
    },
    {
      "path": "pages/tabbar/navigation/index",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTextStyle": "white"
      }
    },
    {
      "path": "pages/tabbar/user/index",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTextStyle": "white"
      }
    },
    {
      "path": "pages/tabbar/collect/index",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTextStyle": "white"
      }
    },
    {
      "path": "pages/login/login",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTextStyle": "white"
      }
    },
    {
      "path": "pages/login/register",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTextStyle": "white"
      }
    },
    {
      "path": "pages/destination/index",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTextStyle": "white"
      }
    },
    {
      "path": "pages/search/index",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTextStyle": "white"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#fff",
    "backgroundColor": "#fff",
    "app-plus": {
      "scrollIndicator": "none"
    }
  },
  "tabBar": {
    "color": "white",
    "backgroundColor": "#1b3232",
    "selectedColor": "#D6A266",
    "list": [
      {
        "pagePath": "pages/tabbar/home/index",
        "iconPath": "./static/images/tabBar/home.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/tabbar/navigation/index",
        "iconPath": "./static/images/tabBar/compass.png",
        "text": "导航"
      },
      {
        "pagePath": "pages/tabbar/collect/index",
        "iconPath": "./static/images/tabBar/bookmark.png",
        "text": "收藏"
      },
      {
        "pagePath": "pages/tabbar/user/index",
        "iconPath": "./static/images/tabBar/user.png",
        "text": "我的"
      }
    ]
  }
}